<AnchorPane prefHeight="385.0" prefWidth="560.0" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.groupproject.Staff">
   <children>
      <ImageView fitHeight="112.0" fitWidth="131.0" layoutX="14.0" layoutY="77.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../../image/Staff.png" />
         </image>
      </ImageView>
      <ImageView fitHeight="78.0" fitWidth="54.0" layoutX="504.0" layoutY="14.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../../image/profile-user.png" />
         </image>
      </ImageView>
      <Label layoutX="23.0" layoutY="23.0" text="Staff Dashboard">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <Button fx:id="profileButton" layoutX="505.0" layoutY="77.0" mnemonicParsing="false" onAction="#Profile" text="Profile" />
      <Button fx:id="dashboardButton" layoutX="50.0" layoutY="214.0" mnemonicParsing="false" onAction="#Dashboard" text="Dashboard">
         <font>
            <Font size="18.0" />
